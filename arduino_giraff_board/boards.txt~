##############################################################

giraff1.name=Giraff ATMega664 Board at 16MHz / STK500 
giraff1.upload.maximum_size=63488
giraff1.upload.speed=38400
giraff1.upload.protocol=stk500
giraff1.bootloader.low_fuses=0xE6
giraff1.bootloader.high_fuses=0xDC
giraff1.bootloader.extended_fuses=0xFD
giraff1.bootloader.path=atmega644
giraff1.bootloader.file=Giraff-ATmega644.hex
giraff1.bootloader.unlock_bits=0x3F
giraff1.bootloader.lock_bits=0x0F
giraff1.build.mcu=atmega644
giraff1.build.f_cpu=16000000L
giraff1.build.core=atmega644
#giraff1.build.variant=giraff

##############################################################

